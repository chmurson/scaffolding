{
  "properties": {
    "name": {
      "pattern": "^[a-zA-Z\\.\\-\\d]+$",
      "message": "Name must be only letters, numbers, dots, or dashes",
      "default": "MyNewComponent",
      "required": true
    }
  },
  "tpl": {
    "tpl/redux-saga/Component.jsx": "{{name}}/{{name}}.jsx",
    "tpl/redux-saga/ComponentContainer.js": "{{name}}/{{name}}Container.js",
    "tpl/redux-saga/duck.js": "{{name}}/duck.js",
    "tpl/redux-saga/selectors.js": "{{name}}/selectors.jsx",
    "tpl/redux-saga/index.js": "{{name}}/index.jsx",
    "tpl/redux-saga/spec/Component--spec.jsx": "{{name}}/spec/{{name}}--spec.jsx",
    "tpl/redux-saga/spec/ComponentContainer--spec.js": "{{name}}/spec/{{name}}Container--spec.js",
    "tpl/redux-saga/spec/reducer--spec.js": "{{name}}/spec/reducer--spec.js",
    "tpl/redux-saga/spec/saga--spec.js": "{{name}}/spec/saga--spec.js"
  }
}
